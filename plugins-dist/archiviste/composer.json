{
    "name": "spip/archiviste",
    "description": "GÃ©rer des archives de fichiers Zip, Tar, ...",
    "license": "GPL-3.0-or-later",
    "type": "spip-plugin",
    "keywords": [
        "zip",
        "tar",
        "archives"
    ],
    "readme": "https://git.spip.net/spip/archiviste/src/branch/master/README.md",
    "authors": [
        {
            "name": "SPIP Team",
            "homepage": "https://discuter.spip.net/c/spip-dev/5",
            "role": "Maintainer"
        }
    ],
    "homepage": "https://git.spip.net/spip/archiviste",
    "support": {
        "issues": "https://git.spip.net/spip/archiviste/issues"
    },
    "autoload": {
        "psr-4": {
            "Spip\\Archiver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spip\\Archiver\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-zip": "*",
        "ext-zlib": "*",
        "ext-phar": "*",
        "symfony/polyfill-php80": "^1.27",
        "symfony/polyfill-php81": "^1.27",
        "symfony/polyfill-php82": "^1.27"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "spip/coding-standards": "^1.2",
        "phpstan/phpstan": "^1.4",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "rector/rector": "^0.12.13"
    },
    "config": {
        "platform": {
            "php": "7.4.27"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    }
}
